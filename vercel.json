{
  "version": 2,
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "regions": [
    "gru1"
  ],
  "functions": {
    "api/index.js": {
      "maxDuration": 300
    }
  },
  "redirects": [
    {
      "source": "/",
      "destination": "/api",
      "permanent": false
    },
    {
      "source": "/index.js",
      "destination": "/api",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    }
  ]
}